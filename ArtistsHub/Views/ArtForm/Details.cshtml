
@model ArtistsHub.Models.ViewModel.DetailArtForm
@{
    ViewBag.Title = "Details";
}
<h4><a href="/artform/list">Go back to list</a></h4>


<h4><a href="/artform/update/@Model.selectedArtForm.ArtFormID">Update this.</a></h4>

<h4><a href="/artform/deleteConfirmation/@Model.selectedArtForm.ArtFormID">Delete this.</a></h4>
<h2>Details</h2>

<h3>@Model.selectedArtForm.ArtFormName</h3>
<p> Discipline Name : <a href="/discipline/details/@Model.selectedArtForm.DisciplineId"> @Model.selectedArtForm.DisciplineName</a></p>
<h3>Related Atists</h3>
<ul>
    @foreach(var artist in Model.relatedArtist)
    {
        <li> <a href="/artist/details/@artist.ArtistID">@artist.ArtistFirstName @artist.ArtistLastName</a></li>
    }
</ul>


