
@{
    ViewBag.Title = "New";
}

<h2>Create a Art Form</h2>

<form action="/ArtForm/Create" method="POST">
    <div>
        <label>Art Form Name:</label> <input type="text" name="ArtFormName" />
    </div>
    <div>
        <label>Discipline Id:</label> <input type="number" name="DisciplineID" />
    </div>
  
    <div style="padding:1em;">
        <input type="submit" value="Add Art Form" />

    </div>
</form>


